<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 配置驱动 -->
	<classPathEntry location="T:\Study\Java\EclipseJee\mysql-connector-java-5.1.45-bin.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/tourpal" userId="root"
			password="123" />
		<!-- 生成实体类 实体bean文件 -->
		<javaModelGenerator targetPackage="com.xchen.po"
			targetProject="cqtourpal" />
		<!-- mapper xml文件 -->
		<sqlMapGenerator targetPackage="com.xchen.mapper"
			targetProject="cqtourpal" />
		<!-- mapper 接口文件 -->
		<javaClientGenerator targetPackage="com.xchen.mapper"
			targetProject="cqtourpal" type="XMLMAPPER" />
		<table schema="" tableName="user_info" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table schema="" tableName="view_info" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>
	</context>
</generatorConfiguration>